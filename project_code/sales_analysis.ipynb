import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
data = pd.read_csv('C:\\Users\\Administrator\\Desktop\\miniproject\\data\\sales_data.csv')
data.head()
data['Date'] = pd.to_datetime(data['Date'])
data.drop_duplicates(inplace=True)
top_products = data.groupby('Product')['Sales'].sum().sort_values(ascending=False).head(5)
top_products.plot(kind='bar', title='Top 5 Products by Sales')
plt.show()
monthly_sales = data.groupby(data['Date'].dt.to_period('M'))['Sales'].sum()
monthly_sales.plot(title='Monthly Sales Trend', marker='o')
plt.show()
